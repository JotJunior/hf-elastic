{
  "name": "jot/hf-elastic",
  "description": "Elasticsearch service and migrations using etcd credentials for connection.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "elasticsearch",
    "etcd"
  ],
  "autoload": {
    "psr-4": {
      "Jot\\HfElastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jot\\HfElastic\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Joao Zanon"
    }
  ],
  "extra": {
    "hyperf": {
      "config": "Jot\\HfElastic\\ConfigProvider"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/command": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/di": "~3.1.0",
    "hyperf/event": "^3.1",
    "hyperf/framework": "^3.1",
    "ramsey/uuid": "^4.7",
    "jot/hf-utils": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "test": "phpunit --testdox",
    "coverage": "phpunit --testdox --coverage-html=coverage/ --coverage-filter=src/",
    "coverage-text": "phpunit --testdox --coverage-text --coverage-filter=src/"
  }
}
