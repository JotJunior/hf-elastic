{
  "name": "jot/hf-elastic",
  "version": "0.11.0",
  "description": "Elasticsearch service and migrations using etcd credentials for connection.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "elasticsearch",
    "etcd"
  ],
  "autoload": {
    "psr-4": {
      "Jot\\HfElastic\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joao Zanon"
    }
  ],
  "extra": {
    "hyperf": {
      "config": "Jot\\HfElastic\\ConfigProvider"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/elasticsearch": "~3.1.0",
    "hyperf/etcd": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/command": "~3.1.0",
    "hyperf/di": "~3.1.0",
    "hyperf/event": "^3.1",
    "hyperf/framework": "^3.1"
  },
  "scripts": {
    "test": "co-phpunit -c phpunit.xml --colors=always"
  }
}
