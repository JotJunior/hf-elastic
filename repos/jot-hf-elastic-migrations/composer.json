{
    "name": "jot/hf-elastic-migrations",
    "description": "Migration components for Elasticsearch integration with HyperF 3.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jot",
            "email": "contato@jot.com.br"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/framework": "3.1.*",
        "hyperf/di": "3.1.*",
        "hyperf/config": "3.1.*",
        "hyperf/command": "3.1.*",
        "hyperf/stringable": "3.1.*",
        "elasticsearch/elasticsearch": "^7.0",
        "jot/hf-elastic-core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jot\\HfElasticMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jot\\HfElasticMigrations\\Test\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Jot\\HfElasticMigrations\\ConfigProvider"
        }
    },
    "minimum-stability": "stable"
}
